@model Telerik.Sitefinity.Frontend.Forms.Mvc.Models.Fields.PageBreak.PageBreakViewModel

@using Telerik.Sitefinity.Frontend.Mvc.Helpers;
@using Telerik.Sitefinity.Modules.Pages;
@Html.Script(ScriptRef.JQuery, "jquery", false)
<div class="@Model.CssClass my-3">
    <button data-sf-btn-role="next" type="button" class="btn btn-secondary">@Html.HtmlSanitize(Model.NextStepText)</button>
    @if (Model.AllowGoBack)
    {
        <a href="javascript:void(0)" class="ms-2" data-sf-btn-role="prev">@Html.HtmlSanitize(Model.PreviousStepText)</a>
    }
</div>
@Html.Script(Url.WidgetContent("Mvc/Scripts/Form/form.all.js"), "bottom", false)
